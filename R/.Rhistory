temp3  <- cbind(type3,adf_results[[i]][[3]][,c(1,3)])
res <- rbind(res,temp1,temp2,temp3)
index_name <- append(index_name,rep(names(list)[i],
(length(type1)+length(type2)+length(type3))))
}
